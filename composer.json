{
    "name": "xden/artgui",
    "description": "Laravel Artisan GUI devtools",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xden",
            "email": "mail@xden.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Xden\\ArtGui\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xden\\ArtGui\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xden\\ArtGui\\ArtGuiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
